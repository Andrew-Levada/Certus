<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="200dp"
    android:height="200dp"
    android:viewportWidth="500"
    android:viewportHeight="500"
    android:name="vector">
    <group android:name="vector_group">
        <group android:name="top">
            <path
                android:pathData="M250,80 320,230 250,270 180,230 250,80"
                android:strokeColor="#ffffff"
                android:strokeWidth="10"
                android:strokeLineCap="round"
                android:strokeLineJoin="round"
                android:name="top_shape" />
            <path
                android:pathData="M250,80 250,270"
                android:strokeColor="#ffffff"
                android:strokeWidth="10"
                android:strokeLineCap="round"
                android:strokeLineJoin="round"
                android:name="top_line" />
        </group>

        <group android:name="bottom">
            <path
                android:pathData="M250,300 320,260 250,390 180,260 250,300"
                android:strokeColor="#ffffff"
                android:strokeWidth="10"
                android:strokeLineCap="round"
                android:strokeLineJoin="round"
                android:name="bottom_shape" />
            <path
                android:pathData="M250,300 250,390"
                android:strokeColor="#ffffff"
                android:strokeWidth="10"
                android:strokeLineCap="round"
                android:strokeLineJoin="round"
                android:name="bottom_line" />
        </group>
    </group>
</vector>
