<?xml version="1.0" encoding="utf-8"?>
<animated-vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:drawable="@drawable/ic_logo">
    <target android:name="vector" >
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:duration="500"
                    android:propertyName="alpha"
                    android:valueFrom="0"
                    android:valueTo="1"
                    android:interpolator="@anim/fast_out_slow_in_interpolator"/>
            </set>
        </aapt:attr>
    </target>

    <target android:name="vector_group" >
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:duration="3000"
                    android:propertyName="translateY"
                    android:valueFrom="80"
                    android:valueTo="-80"
                    android:interpolator="@anim/fast_out_slow_in_interpolator"/>

                <objectAnimator
                    android:duration="3000"
                    android:startOffset="4000"
                    android:propertyName="translateY"
                    android:valueFrom="-80"
                    android:valueTo="80"
                    android:interpolator="@anim/fast_out_slow_in_interpolator"/>
            </set>
        </aapt:attr>
    </target>

    <target android:name="top_shape" >
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:duration="3000"
                    android:propertyName="pathData"
                    android:valueFrom="M250,80 320,230 250,270 180,230 250,80"
                    android:valueTo="M250,80 320,230 250,190 180,230 250,80"
                    android:valueType="pathType"
                    android:interpolator="@anim/fast_out_slow_in_interpolator"/>

                <objectAnimator
                    android:duration="3000"
                    android:startOffset="4000"
                    android:propertyName="pathData"
                    android:valueFrom="M250,80 320,230 250,190 180,230 250,80"
                    android:valueTo="M250,80 320,230 250,270 180,230 250,80"
                    android:valueType="pathType"
                    android:interpolator="@anim/fast_out_slow_in_interpolator"/>
            </set>
        </aapt:attr>
    </target>

    <target android:name="top_line" >
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:duration="3000"
                    android:propertyName="pathData"
                    android:valueFrom="M250,80 250,270"
                    android:valueTo="M250,80 250,190"
                    android:valueType="pathType"
                    android:interpolator="@anim/fast_out_slow_in_interpolator"/>

                <objectAnimator
                    android:duration="3000"
                    android:startOffset="4000"
                    android:propertyName="pathData"
                    android:valueFrom="M250,80 250,190"
                    android:valueTo="M250,80 250,270"
                    android:valueType="pathType"
                    android:interpolator="@anim/fast_out_slow_in_interpolator"/>
            </set>
        </aapt:attr>
    </target>

    <target android:name="bottom_shape" >
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:duration="3000"
                    android:propertyName="pathData"
                    android:valueFrom="M250,300 320,260 250,390 180,260 250,300"
                    android:valueTo="M250,220 320,260 250,390 180,260 250,220"
                    android:valueType="pathType"
                    android:interpolator="@anim/fast_out_slow_in_interpolator"/>

                <objectAnimator
                    android:duration="3000"
                    android:startOffset="4000"
                    android:propertyName="pathData"
                    android:valueFrom="M250,220 320,260 250,390 180,260 250,220"
                    android:valueTo="M250,300 320,260 250,390 180,260 250,300"
                    android:valueType="pathType"
                    android:interpolator="@anim/fast_out_slow_in_interpolator"/>
            </set>
        </aapt:attr>
    </target>

    <target android:name="bottom_line" >
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:duration="3000"
                    android:propertyName="pathData"
                    android:valueFrom="M250,300 250,390"
                    android:valueTo="M250,220 250,390"
                    android:valueType="pathType"
                    android:interpolator="@anim/fast_out_slow_in_interpolator"/>

                <objectAnimator
                    android:duration="3000"
                    android:startOffset="4000"
                    android:propertyName="pathData"
                    android:valueFrom="M250,220 250,390"
                    android:valueTo="M250,300 250,390"
                    android:valueType="pathType"
                    android:interpolator="@anim/fast_out_slow_in_interpolator"/>
            </set>
        </aapt:attr>
    </target>
</animated-vector>